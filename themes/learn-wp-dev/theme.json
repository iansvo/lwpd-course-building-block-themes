		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"color": {
			"background": "var(--wp--preset--color--base)",
			"text": "var(--wp--preset--color--contrast)"
		},
		"elements": {
			"button": {
				":hover": {
					"color": {
						"background": "color-mix(in oklab, var(--wp--preset--color--primary), #fff 20%)"
					}
				},
				"border": {
					"radius": "10px"
				},
				"color": {
					"background": "var(--wp--preset--color--primary)",
					"text": "var(--wp--preset--color--contrast)"
				},
				"spacing": {
					"padding": {
						"bottom": "var(--wp--preset--spacing--x-small)",
						"left": "var(--wp--preset--spacing--small)",
						"right": "var(--wp--preset--spacing--small)",
						"top": "var(--wp--preset--spacing--x-small)"
					}
				}
			},
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-large)",
					"lineHeight": "1"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)",
					"lineHeight": "1.11"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)",
					"lineHeight": "1.2"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--base)",
					"lineHeight": "1.33"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--base)"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"heading": {
				"color": {
					"text": "var(--wp--preset--color--contrast)"
				},
				"typography": {
					"fontStyle": "normal",
					"fontWeight": "600",
					"letterSpacing": "-0.01em",
					"lineHeight": "1.45"
				}
			},
			"link": {
				":hover": {
					"color": {
						"text": "var(--wp--preset--color--contrast)"
					}
				},
				"color": {
					"text": "var(--wp--preset--color--primary)"
				}
			}
		},
		"spacing": {
			"padding": {
				"bottom": "0px",
				"left": "var(--wp--preset--spacing--large)",
				"right": "var(--wp--preset--spacing--large)",
				"top": "0px"
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--onest)",
			"fontSize": "var(--wp--preset--font-size--base)",
			"lineHeight": "1.5"
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header"
		},
		{
			"area": "footer",
			"name": "footer"
		}
	],
	"version": 3,
	"$schema": "https://schemas.wp.org/wp/6.8/theme.json"
}